#: ui.py:1102
#, python-format
msgid ""
"Server URL:    %s\n"
"\n"
"<   >      =   Prognosis next/previous day\n"
"0 - 9      =   Prognosis (x) days from now\n"
"\n"
"VOL+/-     =   Fast scroll 100 (City choice)\n"
"Bouquet+/- =   Fast scroll 500 (City choice)\n"
"\n"
"Info       =   This information\n"
"Menu       =   Satellite photos and maps\n"
"\n"
"Red        =   Temperature chart for the upcoming 5 days\n"
"Green      =   Go to Favorite 1\n"
"Yellow     =   Go to Favorite 2\n"
"Blue       =   Go to Home\n"
"\n"
"Wind direction =   Arrow to right: Wind from the West\n"
msgstr ""
"Server URL:    %s\n"
"\n"
"<   >       =   Prossime previsioni/giorno precedente\n"
"0 - 9       =   Previsioni (x) giorni da ora \n"
"\n"
"VOL+/-  =   Scorrimento veloce 100 (Città scelta)\n"
"Bouquet+/- =  Scorrimento veloce 500 (Città scelta)\n"
"\n"
"Info        =   Questa info\n"
"Menu     =   Satellite foto e mappe \n"
"\n"
"Red        =   Mappa Temperatura prossimi 5 giorni \n"
"Green    =   Vai a Favoriti 1\n"
"Yellow    =   Vai a Favoriti 2\n"
"Blue        =   Vai a inizio \n"
"\n"
"Wind direction = Freccia a destra: Vento da Ovest\n"

